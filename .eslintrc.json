{
    "extends": ["next", "next/typescript", "prettier"],
    "rules": {
        "react/no-unescaped-entities": "off",
        "@next/next/no-page-custom-font": "off",
        "no-restricted-imports": [
            "error",
            {
                "patterns": [{ "regex": "^@mui/[^/]+$" }]
            }
        ]
    }
}
