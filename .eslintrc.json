{
    "extends": ["next", "next/typescript", "prettier"],
    "plugins": ["@typescript-eslint"],
    "rules": {
        "react/no-unescaped-entities": "off",
        "@next/next/no-page-custom-font": "off",
        "@typescript-eslint/no-explicit-any": ["warn"],
        "no-restricted-imports": [
            "error",
            {
                "patterns": [{ "regex": "^@mui/[^/]+$" }]
            }
        ]
    }
}
